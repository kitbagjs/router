{
  "name": "@kitbag/router",
  "private": false,
  "version": "0.20.2",
  "bugs": {
    "url": "https://github.com/kitbagjs/router/issues"
  },
  "keywords": [
    "typescript",
    "router",
    "vue",
    "vue-router",
    "types",
    "typed"
  ],
  "homepage": "https://github.com/kitbagjs/router#readme",
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch --minify=false",
    "test": "vitest --typecheck",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "types": "vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "vite build && typedoc && sleep 5 && node ./scripts/api.js"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/kitbag-router.umd.cjs",
  "module": "./dist/kitbag-router.js",
  "types": "./dist/kitbag-router.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kitbag-router.js",
      "require": "./dist/kitbag-router.umd.cjs"
    }
  },
  "devDependencies": {
    "@kitbag/eslint-config": "1.0.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.8.3",
    "valibot": "^1.0.0",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "zod": "^3.24.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "history": "^5.3.0"
  }
}
