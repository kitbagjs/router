{
  "name": "@kitbag/router",
  "private": false,
  "version": "0.12.0",
  "bugs": {
    "url": "https://github.com/kitbagjs/router/issues"
  },
  "homepage": "https://github.com/kitbagjs/router#readme",
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "test": "vitest --typecheck",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "types": "vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "typedoc"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/kitbag-router.umd.cjs",
  "module": "./dist/kitbag-router.js",
  "types": "./dist/kitbag-router.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kitbag-router.js",
      "require": "./dist/kitbag-router.umd.cjs"
    }
  },
  "devDependencies": {
    "@kitbag/eslint-config": "1.0.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "happy-dom": "^15.11.7",
    "typedoc": "^0.27.2",
    "typedoc-plugin-markdown": "^4.2.10",
    "typedoc-plugin-vue": "^1.2.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.6",
    "vue-tsc": "^2.1.10"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "history": "^5.3.0"
  }
}
